<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bob.todo_list">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_todo_list"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_todo_list"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyTodoApplication">
        <activity android:name="com.bob.todo_list.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>